{
  "name": "qambi",
  "private": false,
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-3": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "eslint": "^2.7.0",
    "eslint-plugin-babel": "^3.2.0",
    "flow-bin": "^0.22.1",
    "watchify": "^3.7.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "license": "MIT",
  "repository": "git@github.com:abudaan/qambi.git",
  "babel": {
    "plugins": [
      "transform-object-rest-spread",
      "transform-flow-strip-types"
    ],
    "presets": [
      "es2015"
    ]
  },
  "engines": {
    "node": "0.10.x"
  },
  "scripts": {
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "test": "babel-node ./src/fp/test.js",
    "start": "webpack-dev-server",
    "watch": "watchify src/test_web.js -t babelify -o src/build/app.js --debug --verbose",
    "build": "browserify test_web.js -t babelify --debug | exorcist build/app.js.map | uglifyjs -c > build/app.js"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "redux": "^3.3.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1"
  }
}
