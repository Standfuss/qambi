{
  "name": "qambi",
  "version": "1.0.0-beta5",
  "description": "MIDI sequencer, loads MIDI files, can record and playback MIDI, uses WebMIDI and WebAudio",
  "author": "abudaan daniel@abumarkub.net",
  "main": "dist/qambi.js",
  "private": false,
  "devDependencies": {
    "babel-eslint": "^6.0.2",
    "babel-plugin-transform-es2015-modules-amd": "6.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "eslint": "^2.7.0",
    "eslint-plugin-babel": "^3.2.0",
    "gulp": "3.9.1",
    "gulp-uglify": "1.5.3",
    "minifyify": "7.3.3",
    "vinyl-source-stream": "1.1.0",
    "watchify": "^3.7.0"
  },
  "license": "MIT",
  "repository": "git@github.com:abudaan/qambi.git",
  "babel": {
    "plugins": [
      "transform-flow-strip-types"
    ],
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "engines": {
    "node": ">0.10.x"
  },
  "scripts": {
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "watch": "watchify ./onscreen-keyboard/index.js -t babelify -o ./onscreen-keyboard/build.js --debug --verbose"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "qambi": "1.0.0-beta16"
  }
}
